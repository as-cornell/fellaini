{# {{kint()}} #}

{# 
    Template to show the single text field from a paragraph. To get to the specific value to be output use. 

    item.content['#context'].value | striptags

 #}

{% for item in items %}
    {% include "@atoms/headings/_heading.twig" with {
        "heading_level" : 2,
        "heading" : item.content['#context'].value | striptags,
        "heading_modifiers" : ["margin"]
    } %}

{% endfor %}